LoopPlay.MouseButton1Down:Connect(function()
while wait() do 
    for i,tool in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if tool:IsA('Tool') then
            for _,sound in pairs(tool:GetDescendants()) do
                if sound:IsA'Sound' then
                    sound.Playing = true
                end
            end
        end
    end
end
end)
